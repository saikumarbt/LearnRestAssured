{
  "definitions": {},
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://example.com/root.json",
  "type": "object",
  "title": "The Root Schema",
  "required": [
    "id",
    "name",
    "releaseDate",
    "reviewScore",
    "category",
    "rating"
  ],
  "properties": {
    "id": {
      "$id": "#/properties/id",
      "type": "integer",
      "title": "The Id Schema",
      "default": 0,
      "examples": [
        5
      ]
    },
    "name": {
      "$id": "#/properties/name",
      "type": "string",
      "title": "The Name Schema",
      "default": "",
      "examples": [
        "The Legend of Zelda: Ocarina of Time"
      ],
      "pattern": "^(.*)$"
    },
    "releaseDate": {
      "$id": "#/properties/releaseDate",
      "type": "string",
      "title": "The Releasedate Schema",
      "default": "",
      "examples": [
        "1998-12-10"
      ],
      "pattern": "^(.*)$"
    },
    "reviewScore": {
      "$id": "#/properties/reviewScore",
      "type": "integer",
      "title": "The Reviewscore Schema",
      "default": 0,
      "examples": [
        93
      ]
    },
    "category": {
      "$id": "#/properties/category",
      "type": "string",
      "title": "The Category Schema",
      "default": "",
      "examples": [
        "Adventure"
      ],
      "pattern": "^(.*)$"
    },
    "rating": {
      "$id": "#/properties/rating",
      "type": "string",
      "title": "The Rating Schema",
      "default": "",
      "examples": [
        "PG-13"
      ],
      "pattern": "^(.*)$"
    }
  }
}